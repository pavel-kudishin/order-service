{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"Kestrel": {
		"Endpoints": {
			"Rest": {
				"Url": "http://*:50001",
				"Protocols": "Http1AndHttp2"
			},
			"Rest80": {
				"Url": "http://*:80",
				"Protocols": "Http1AndHttp2"
			},
			"gRPC": {
				"Url": "http://*:50002",
				"Protocols": "Http2"
			}
		}
	}
}